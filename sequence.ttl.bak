@prefix prov: <http://www.w3.org/ns/prov#> . 
@prefix qudt: <http://qudt.org/schema/qudt/>.

@prefix emobon-core: <{{baseuri}}/core#> .
@prefix emobon-sampling: <{{baseuri}}/sampling#> .
@prefix emobon-sequence: <{{baseuri}}/{{name}}#> .

