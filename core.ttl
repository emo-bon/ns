### CLASSES Core ###
schema:Thing
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty schema:identifier ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange <http://www.w3.org/ns/shacl#IRI>
  ], [
    a owl:Restriction ;
    owl:onProperty schema:description ;
    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange xsd:string ;
  ] ;
.


<https://data.emobon.embrc.eu/ns/core#Observatory>
  a owl:Class ;
  rdfs:subClassOf schema:Thing, [
    a owl:Restriction ;
    owl:onProperty <https://data.emobon.embrc.eu/ns/core#broadBiome> ;
    owl:someValuesFrom <http://purl.obolibrary.org/obo/ENVO_00000428> ;
  ], [
    a owl:Restriction ;
    owl:onProperty <https://data.emobon.embrc.eu/ns/core#localBiome> ;
    owl:someValuesFrom <http://purl.obolibrary.org/obo/ENVO_00000428> ;
  ], [
    a owl:Restriction ;
    owl:onProperty <https://data.emobon.embrc.eu/ns/core#geoLocation> ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass <http://www.opengis.net/ont/sf#Point> ;
  ], [
    a owl:Restriction ;
    owl:onProperty <https://data.emobon.embrc.eu/ns/core#sampleType> ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass <https://data.emobon.embrc.eu/ns/core#SampleType> ;
  ], [
    a owl:Restriction ;
    owl:onProperty <https://data.emobon.embrc.eu/ns/core#marineRegion> ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange xsd:anyURI ;
  ], [
    a owl:Restriction ;
    owl:onProperty <https://data.emobon.embrc.eu/ns/core#observatoryId> ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange xsd:string ;
  ], [
    a owl:Restriction ;
    owl:onProperty <https://data.emobon.embrc.eu/ns/core#observatorySampleId> ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange xsd:string ;
  ] ;
.

<http://purl.obolibrary.org/obo/ENVO_00000428> 
  a owl:Class ;
.


<https://data.emobon.embrc.eu/ns/core#SampleType>
  a owl:Class ;
  owl:equivalentClass [
    a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    owl:oneOf (
     <https://data.emobon.embrc.eu/ns/core#Hard>
     <https://data.emobon.embrc.eu/ns/core#Soil>
     <https://data.emobon.embrc.eu/ns/core#Water>
   )
  ] ;
.

<https://data.emobon.embrc.eu/ns/core#Hard> 
  a owl:NamedIndividual, <https://data.emobon.embrc.eu/ns/core#SampleType> ;
.

<https://data.emobon.embrc.eu/ns/core#Soil> 
  a owl:NamedIndividual, <https://data.emobon.embrc.eu/ns/core#SampleType> ;
.

<https://data.emobon.embrc.eu/ns/core#Water> 
  a owl:NamedIndividual, <https://data.emobon.embrc.eu/ns/core#SampleType> ;
.

schema:Organization
  a owl:Class ;
  rdfs:subClassOf schema:Thing ;
.

<http://www.embrc.eu/emo-bon/EmoBonOntology.rdf#Organization>
  a owl:Class ;
  rdfs:subClassOf schema:Organization ;
.


schema:PostalAddress
  a owl:Class ;
  rdfs:subClassOf schema:ContactPoint, [
    a owl:Restriction ;
    owl:onProperty schema:addressCountry ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange xsd:string ;
  ], [
    a owl:Restriction ;
    owl:onProperty schema:postalCode ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange xsd:string ;
  ], [
    a owl:Restriction ;
    owl:onProperty schema:streetAddress ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange xsd:string ;
  ] ;
.

schema:ContactPoint 
  a owl:Class ;
.

<http://www.opengis.net/ont/sf#Geometry>
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty geo:asWKT ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onDataRange geo:wktLiteral ;
  ] ;
.

<http://www.opengis.net/ont/sf#Point>
  a owl:Class ;
  rdfs:subClassOf <http://www.opengis.net/ont/sf#Geometry> ;
.


### PROPERTIES Core ###
<https://data.emobon.embrc.eu/ns/core#hasBiome>
  a owl:ObjectProperty ;
  rdfs:domain <https://data.emobon.embrc.eu/ns/core#Observatory> ;
  rdfs:range <http://purl.obolibrary.org/obo/ENVO_00000428> ;
.

<https://data.emobon.embrc.eu/ns/core#broadBiome>
  a owl:ObjectProperty ;
  rdfs:subPropertyOf <https://data.emobon.embrc.eu/ns/core#hasBiome> ;
.

<https://data.emobon.embrc.eu/ns/core#localBiome>
  a owl:ObjectProperty ;
  rdfs:subPropertyOf <https://data.emobon.embrc.eu/ns/core#hasBiome> ;
.

<https://data.emobon.embrc.eu/ns/core#geoLocation>
  a owl:ObjectProperty ;
  rdfs:range <http://www.opengis.net/ont/sf#Point> ;
.

<https://data.emobon.embrc.eu/ns/core#sampleType>
  a owl:ObjectProperty ;
  rdfs:range <https://data.emobon.embrc.eu/ns/core#SampleType> ;
.

schema:address
  a owl:ObjectProperty ;
  rdfs:range schema:PostalAddress ;
.


<https://data.emobon.embrc.eu/ns/core#marineRegion>
  a                   owl:DatatypeProperty ;
  rdfs:domain         <https://data.emobon.embrc.eu/ns/core#Observatory> , sosa:Sampling ;
  rdfs:range          xsd:anyURI ;
  owl:seeAlso         <https://dwc.tdwg.org/terms/#dwc:higherGeographyID> , <http://rs.tdwg.org/dwc/terms/locality> ; 
.

<https://data.emobon.embrc.eu/ns/core#marineRegionName>
  a                   owl:DatatypeProperty ;
  rdfs:isDefinedBy    <https://data.emobon.embrc.eu/ns/core> ;
  rdfs:comment        "The name of the marine region"@en ;
  rdfs:label          "Name associated with the marine region."@en ;
  rdfs:domain         <https://data.emobon.embrc.eu/ns/core#Observatory> , sosa:Sampling ;
  rdfs:range          xsd:string ;
  owl:seeAlso         <http://rs.tdwg.org/dwc/terms/higherGeography> , <https://genomicsstandardsconsortium.github.io/mixs/0000010/> , <http://rs.tdwg.org/dwc/terms/locality> ;
.

<https://data.emobon.embrc.eu/ns/core#observatoryId>
  a owl:DatatypeProperty ;
  rdfs:domain <https://data.emobon.embrc.eu/ns/core#Observatory> ;
  rdfs:range xsd:string ;
.

<https://data.emobon.embrc.eu/ns/core#observatorySampleId>
  a owl:DatatypeProperty ;
  rdfs:domain <https://data.emobon.embrc.eu/ns/core#Observatory> ;
  rdfs:range xsd:string ;
.

<https://data.emobon.embrc.eu/ns/core#edmoId>
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain <https://data.emobon.embrc.eu/ns/core#Organization> ;
  rdfs:range xsd:positiveInteger ;
.

<https://data.emobon.embrc.eu/ns/core#waterColumnDepth>
    a                   owl:DatatypeProperty ;
    rdfs:isDefinedBy    <https://data.emobon.embrc.eu/ns/core> ;
    rdfs:comment        "Measument of the total depth of the water column in meters"@en ;
    rdfs:label          "Total depth of water column"@en ;
    rdfs:domain         [
                            a owl:Class ; 
                            owl:unionOf (
                                sosa:Sampling
                                <https://data.emobon.embrc.eu/ns/core#Observatory>
                            ) ; 
                        ] ;
    rdfs:range          qudt:QuantityValue ;
.

<https://data.emobon.embrc.eu/ns/core#originCountry>
    a                   owl:DatatypeProperty ;
    rdfs:isDefinedBy    <https://data.emobon.embrc.eu/ns/core> ;
    rdfs:comment        "The geographical origin of the observatory/sample as defined by the country name."@en ;
    rdfs:label          "Country of origin of the sample"@en ;
    rdfs:domain         [
                            a owl:Class ; 
                            owl:unionOf (
                                sosa:Sampling
                                <https://data.emobon.embrc.eu/ns/core#Observatory>
                            ) ; 
                        ] ;
    rdfs:range          xsd:string ;
.

<https://data.emobon.embrc.eu/ns/core#siteInfo>
  a                     owl:DatatypeProperty ;
  rdfs:isDefinedBy      <https://data.emobon.embrc.eu/ns/core> ;
  rdfs:comment          "Extra information on the sampling site of the observatory, in particular concerning other data collections from the same area, environmental activities or events, etc." ; 
  rdfs:label            "Sampling site information" ; 
  rdfs:domain           <https://data.emobon.embrc.eu/ns/core#Observatory> ;
  rdfs:range            xsd:string ;
.

<https://data.emobon.embrc.eu/ns/core#hasMixsPackage>
    a                   owl:DatatypeProperty ;
    rdfs:isDefinedBy    <https://data.emobon.embrc.eu/ns/core> ;
    rdfs:comment        "The MIxS extension for reporting of measurements and observations obtained from one or more of the observatory environments where a sample was obtained"@en ;
    rdfs:label          "has Mixs Package"@en ;
    rdfs:domain         <https://data.emobon.embrc.eu/ns/core#Observatory> ;
    rdfs:range          xsd:string ;
.

<https://data.emobon.embrc.eu/ns/core#soilType>
    a                   owl:DatatypeProperty ;
    rdfs:isDefinedBy    <https://data.emobon.embrc.eu/ns/core> ;
    rdfs:comment        "Information about the sediment type of the sample, collected at an Observatory, based on major constituents"@en ;
    rdfs:label          "Sediment type of a soil sample"@en ;
    rdfs:domain         <https://data.emobon.embrc.eu/ns/core#Observatory> ;
    rdfs:range          xsd:string ;
.

<https://data.emobon.embrc.eu/ns/core#enaProjAccNum>
    a                   owl:DatatypeProperty ;
    rdfs:isDefinedBy    <https://data.emobon.embrc.eu/ns/core> ;
    rdfs:comment        "ENA project accession number associated to this project."@en ;
    rdfs:label          "has ENA project accession number"@en ;
    rdfs:domain         schema:Project ;
    rdfs:range          xsd:string ;
    owl:seeAlso         <http://rs.tdwg.org/dwc/terms/associatedSequences> ;
.

<https://data.emobon.embrc.eu/ns/core#enaStudyAccNum>
    a                   owl:DatatypeProperty ;
    rdfs:isDefinedBy    <https://data.emobon.embrc.eu/ns/core> ;
    rdfs:comment        "ENA study accession number associated to this observatory."@en ;
    rdfs:label          "has ENA study accession number"@en ;
    rdfs:domain         <https://data.emobon.embrc.eu/ns/core#Observatory> ;
    rdfs:range          xsd:string ;
    owl:seeAlso         <http://rs.tdwg.org/dwc/terms/associatedSequences> ;
.

schema:legalName
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain schema:Organization ;
  rdfs:range xsd:string ;
.

geo:asWKT
  a owl:DatatypeProperty ;
  rdfs:domain <http://www.opengis.net/ont/sf#Geometry> ;
  rdfs:range geo:wktLiteral ;
.

schema:addressCountry
  a owl:DatatypeProperty ;
  rdfs:range xsd:string ;
.

schema:description
  a owl:DatatypeProperty ;
  rdfs:range xsd:string ;
.

schema:identifier
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:range xsd:anyURI ;
.

schema:postalCode 
  a owl:DatatypeProperty 
.

schema:streetAddress
  a owl:DatatypeProperty ;
  rdfs:domain schema:PostalAddress ;
  rdfs:range xsd:string ;
.

